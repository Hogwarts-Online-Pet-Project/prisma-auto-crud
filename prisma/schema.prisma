generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model movie {
  id    Int     @id(map: "pk_movie") @default(autoincrement())
  title String? @db.VarChar(100)
}

model fruit {
  name String @id(map: "pk_fruit") @db.VarChar(100)
}
